#include <stdio.h>
#include <stdlib.h>
char* my_strdup(char *src) {
 int len = 0;
 while (*(src + len)) len++;
 char *dest = (char*) malloc((len + 1) * sizeof(char));
 if (!dest) return NULL;
 for (int i = 0; i <= len; i++)
 *(dest + i) = *(src + i);
 return dest;
}
int main() {
 printf("pavan kumar N\n");
 printf("1SV24CS093\n");
 char str[100];
 printf("Enter string: ");
 gets(str);
 char *copy = my_strdup(str);
 printf("Copied string: %s\n", copy);
 free(copy);
 return 0;
}
